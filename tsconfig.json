{
    "compilerOptions": {
      "outDir": "dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "esModuleInterop": true,
      "downlevelIteration": true,
    },
    "include": ["src/**/*", "typings.d.ts"],
    "exclude": ["node_modules", "dist"],
    "typeRoots": ["../node_modules/@types"]
  }
  